        <script>
            $(document).ready(function () {
                $(".language-btn .search-img ").click(function (event) {
                    if ($(".search-input").hasClass("search-show")) {
                        $(".search-input").removeClass("search-show");
                    }
                    else {
                        $(".search-input").addClass("search-show");
                    }
                });
            });
        </script>
        <script type="text/javascript">
            $(document).ready(function () {
                $("#to-top").hide();
                $(window).scroll(function () {
                    var currentPosition = $(window).scrollTop();
                    var documentHeight = $(document).height();
                    var windowHeight = $(window).height();
                    if ((documentHeight > windowHeight) && (currentPosition > (windowHeight / 3))) {
                        $("#to-top").show();
                    } else {
                        $("#to-top").hide();
                    }
                });

                $("#to-top").click(function () {
                    var body = $("html, body");
                    body.stop().animate({ scrollTop: 0 }, 500, 'swing');
                });

                $("div.fixed-text").click(function () {
                    $("div.fixed-side-bar").toggleClass("Shown");
                });

            });
        </script>
