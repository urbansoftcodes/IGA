        <script>
            if (!Modernizr.objectfit) {
                $('.h-speakers-img , .item .h-news-ims-sec, .peakersimg').each(function () {
                    var $container = $(this),
                        imgUrl = $container.find('img').prop('src');
                    if (imgUrl) {
                        $container
